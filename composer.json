{
    "name": "dezodev/blade-mdi-icons",
    "description": "A package to easily make use of Material Design Icons in your Laravel Blade views.",
    "keywords": ["Blade", "Material Design Icons", "Laravel"],
    "homepage": "https://github.com/dezodev/blade-mdi-icons",
    "license": "MIT",
    "authors": [
        {
            "name": "Dezodev"
        }
    ],
    "require": {
        "php": "~8.2.0|~8.3.0",
        "blade-ui-kit/blade-icons": "^1.6",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "BladeUI\\MDIIcons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BladeUI\\MDIIcons\\BladeMDIIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
